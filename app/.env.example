# Firebase Configuration
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# API Endpoints
VITE_ANALYSE_API_URL=https://your-analyse-function-url
VITE_FIND_TICKETS_API_URL=https://your-find-tickets-function-url
VITE_FEATURE_FLAGS_API_URL=https://your-feature-flags-function-url

# RevenueCat Configuration
# Set to "true" to enable in-app purchases, "false" to disable
# When disabled, paywall will not show even if capture limit is reached
VITE_REVENUECAT_ENABLED=false

# RevenueCat API Keys
# Get these from: RevenueCat Dashboard → Projects → Your Project → API Keys
# iOS: Copy the "Public Apple SDK Key"
# Android: Copy the "Public Google SDK Key"
VITE_REVENUECAT_API_KEY_IOS=your-ios-api-key
VITE_REVENUECAT_API_KEY_ANDROID=your-android-api-key

# Developer Mode
# Set to "true" to enable developer-specific features in the app
# This includes crash testing, debug logs, and other development tools
# IMPORTANT: Set to "false" in production builds
VITE_DEVELOPER_MODE_ENABLED=false
